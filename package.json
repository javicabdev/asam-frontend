{
  "name": "asam-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run codegen:watch\"",
    "dev:vite": "vite",
    "dev:clean": "npm run clean:sw && npm run dev",
    "clean:sw": "node scripts/clean-sw.cjs",
    "build": "npm run codegen:safe && tsc && vite build",
    "build:local": "npm run codegen:local && tsc && vite build",
    "build:nobackend": "tsc && vite build",
    "build:safe": "node scripts/build-safe.cjs",
    "build:analyze": "npm run codegen:safe && tsc && vite build --mode analyze",
    "build:report": "npm run build && node scripts/analyze-bundle.cjs",
    "analyze": "node scripts/analyze-bundle.cjs",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:type": "npm run type-check && npm run lint",
    "lint:strict": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:local": "graphql-codegen --config codegen.local.yml",
    "codegen:safe": "node scripts/codegen-safe.cjs",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "generate-icons": "node scripts/generate-icons.js",
    "clean-icons": "node scripts/clean-icons.js",
    "regenerate-icons": "npm run clean-icons && npm run generate-icons",
    "optimize-favicon": "node scripts/optimize-favicon.js",
    "generate-text-favicon": "node scripts/generate-text-favicon.js",
    "generate-asam-favicon": "node scripts/generate-asam-favicon.js",
    "check-icons": "node scripts/check-icons-setup.js",
    "cleanup-favicon-tests": "node scripts/cleanup-favicon-tests.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "ci": "node scripts/ci-check.cjs",
    "ci:strict": "npm run lint:strict && npm run test && npm run build",
    "netlify:validate": "node scripts/validate-netlify.cjs",
    "netlify:build": "npm run netlify:validate && npm run build:safe",
    "pwa:copy-icons": "node scripts/copy-pwa-icons.cjs",
    "netlify:prepare": "npm run pwa:copy-icons && npm run netlify:validate",
    "test:coverage": "vitest --coverage",
    "check:circular": "node scripts/check-circular-deps.cjs",
    "check:all": "npm run lint && npm run check:circular && npm run build:safe"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^5.1.1",
    "@mui/icons-material": "^5.15.2",
    "@mui/material": "^5.15.2",
    "@mui/x-data-grid": "^6.18.6",
    "@mui/x-date-pickers": "^6.18.6",
    "@react-pdf/renderer": "^3.4.4",
    "date-fns": "^2.30.0",
    "graphql": "^16.8.1",
    "i18next": "^23.7.16",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.2",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-i18next": "^14.0.0",
    "react-router-dom": "^6.21.1",
    "recharts": "^2.10.3",
    "workbox-window": "^7.0.0",
    "yup": "^1.6.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@parcel/watcher": "^2.5.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/jspdf": "^1.3.3",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "chalk": "^5.6.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-security": "^3.0.1",
    "jsdom": "^23.0.1",
    "madge": "^8.0.0",
    "prettier": "^3.1.1",
    "sharp": "^0.33.0",
    "terser": "^5.43.1",
    "typescript": "^5.9.0",
    "vite": "^7.1.3",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4"
  }
}
